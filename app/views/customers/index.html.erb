<p style="color: green"><%= notice %></p>

<h1>Customers</h1>
<% a= 5 %>
<% b= 10 %>

<p>
  <%= "o resultado da multiplicação entre #{a} e #{b} é :#{a * b}"%>
</p>


<div id="customers">
  <% @customers.each do |customer| %>
    <%= render customer %>
    <p>
      <%= link_to "Show this customer", customer %>
    </p>
  <% end %>
</div>

<%= link_to "New customer", new_customer_path %>
